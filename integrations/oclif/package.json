{
  "name": "@fig/complete-oclif",
  "description": "Generate Fig completion spec from oclif",
  "version": "3.0.0",
  "author": "Matt Schrage @mattschrage",
  "bugs": "https://github.com/withfig/autocomplete-tools/issues",
  "dependencies": {
    "@oclif/core": "^3.15.0",
    "prettier": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@withfig/autocomplete-types": "workspace:^",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mattschrage/fig-oclif-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "pnpm build && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "publishConfig": {
    "access": "public"
  }
}
