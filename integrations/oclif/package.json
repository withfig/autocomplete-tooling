{
  "name": "@fig/complete-oclif",
  "description": "Generate Fig completion spec from oclif",
  "version": "3.0.0",
  "author": "Matt Schrage @mattschrage",
  "bugs": "https://github.com/withfig/autocomplete-tools/issues",
  "dependencies": {
    "@oclif/command": "^1.8.18",
    "@oclif/config": "^1.18.5",
    "prettier": "^2.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-help": "^3.3.1",
    "@types/node": "^16.11.1",
    "@withfig/autocomplete-types": "^1.5.0",
    "globby": "^11.0.4",
    "ts-node": "^10.3.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mattschrage/fig-oclif-plugin",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "publishConfig": {
    "access": "public"
  }
}
