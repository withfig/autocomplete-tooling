{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015", "DOM"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDir": "src",
    "outDir": "build/",
    "types": ["@withfig/autocomplete-types", "node"]
  },
  "exclude": ["node_modules/", "generate-spec.ts", "test"]
}
