{
  "compilerOptions": {
    "target": "es6",
    "outDir": "lib/",
    "rootDir": "./",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "types": ["@withfig/autocomplete-types", "node"]
  },
  "exclude": ["node_modules/", "lib/", "test"]
}
