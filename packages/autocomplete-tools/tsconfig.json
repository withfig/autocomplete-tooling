{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2015"],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build/",
    "types": ["@withfig/autocomplete-types", "node"]
  },
  "include": ["*.ts", "scripts/*.ts"],
  "exclude": ["**/fixtures/**/*.ts"]
}
