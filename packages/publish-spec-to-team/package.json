{
  "name": "@fig/publish-spec-to-team",
  "version": "1.0.0",
  "description": "Publish a spec to fig team",
  "bin": "./lib/bin.js",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "test:server": "TEST_PORT=3005 ts-node test/server.ts",
    "test:fixtures": "TEST_PORT=3005 BASE_URL=http://localhost:3005 ts-node test/index.ts",
    "test": "npm run test:server & SERVER_PID=$!; sleep 2; npm run test:fixtures; kill $SERVER_PID"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/withfig/autocomplete-tools.git"
  },
  "keywords": [
    "fig",
    "team",
    "spec",
    "plugin",
    "integration",
    "completion"
  ],
  "files": [
    "lib/"
  ],
  "author": "Federico Ciardi <fed.ciardi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/withfig/autocomplete-tools/issues"
  },
  "homepage": "https://github.com/withfig/autocomplete-tools#readme",
  "dependencies": {
    "commander": "^9.2.0",
    "esbuild": "^0.14.38",
    "node-fetch": "^3.2.4",
    "prettier": "^2.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.2",
    "@types/node-fetch": "^2.6.1",
    "express": "^4.18.1",
    "multer": "^1.4.4",
    "picocolors": "^1.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
